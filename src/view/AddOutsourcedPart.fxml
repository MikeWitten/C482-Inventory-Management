<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="550" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddOutsourcedPart">
    <children>
        <Label fx:id="label1" layoutX="27.0" layoutY="39.0" text="Add Part">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <RadioButton fx:id="addParts_InHouseButton" layoutX="146.0" layoutY="39.0" mnemonicParsing="false" onAction="#toInHouse" text="In-House" />
        <RadioButton disable="true" layoutX="300.0" layoutY="39.0" mnemonicParsing="false" selected="true" text="Outsourced" />
        <TextField editable="false" layoutX="135.0" layoutY="113.0" promptText="Auto Gen - Disabled">
            <font>
                <Font name="System Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="nameTxt" layoutX="135.0" layoutY="150.0" />
        <TextField fx:id="stockTxt" layoutX="135.0" layoutY="186.0" />
        <TextField fx:id="priceTxt" layoutX="135.0" layoutY="223.0" />
        <TextField fx:id="maxTxt" layoutX="135.0" layoutY="262.0" />
        <TextField fx:id="companyNameTxt" layoutX="135.0" layoutY="300.0" />
        <TextField fx:id="minTxt" layoutX="374.0" layoutY="262.0" />
        <Label layoutX="30.0" layoutY="117.0" text="ID" />
        <Label layoutX="30.0" layoutY="154.0" text="Name" />
        <Label layoutX="30.0" layoutY="190.0" text="Inv" />
        <Label layoutX="30.0" layoutY="227.0" text="Price/Cost" />
        <Label layoutX="30.0" layoutY="266.0" text="Max" />
        <Label layoutX="30.0" layoutY="304.0" text="Company Name" />
        <Label layoutX="314.0" layoutY="266.0" text="Min" />
        <ButtonBar layoutX="274.0" layoutY="360.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button fx:id="addPartsOutsourced_SaveButton" mnemonicParsing="false" onAction="#savePart" text="Save" />
                <Button fx:id="addPartsOutsourced_CancelButton" layoutX="135.0" layoutY="17.0" mnemonicParsing="false" onAction="#backToMain" text="Cancel" />
            </buttons>
        </ButtonBar>
    </children>

</AnchorPane>
